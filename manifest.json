{
    "name": "Cicero Content Grabber",
    "description": "Add custom URLS to the Cicero app.",
    "version": "1.0.1",
    "manifest_version": 3,
    "icons": {
        "16": "img/popup-icons/icons8-greek-pillar-base-16.png",
        "32": "img/popup-icons/icons8-greek-pillar-base-32.png",
        "48": "img/popup-icons/icons8-greek-pillar-base-48.png",
        "64": "img/popup-icons/icons8-greek-pillar-base-64.png",
        "128": "img/popup-icons/icons8-greek-pillar-base-128.png"
    },
    "permissions": ["tabs", "storage"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_title": "Cicero Content Grabber",
        "default_popup": "index.html"
    },
    "options_page": "options.html",
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+J",
                "mac": "MacCtrl+Shift+J"
            }
        }
    }
}